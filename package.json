{
  "name": "console-chess",
  "version": "1.0.0",
  "description": "a simple chess app ",
  "main": "lib/index.js",
  "scripts": {
    "build-config": "babel webpack.config.dev.js --out-file babel.webpack.config.dev.js",
    "pack": "webpack --config babel.webpack.config.dev.js",
    "build": "npm-run-all build-config pack",
    "start": "npm-run-all build launch-app",
    "launch-app": "node app.js testFile.txt",
    "test": "npm-run-all compile-app  run-tests",
    "run-tests": "mocha \"lib/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gxldCptRick/ConsoleChess.git"
  },
  "keywords": [
    "chess"
  ],
  "author": "Andres Carrera, Fernando Blanco",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gxldCptRick/ConsoleChess/issues"
  },
  "homepage": "https://github.com/gxldCptRick/ConsoleChess#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "electron": "^2.0.6",
    "node": "^10.8.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
